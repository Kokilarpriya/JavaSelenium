package com.TechForum.JavaPrograms;

public class Answer {

	public Answer() {
		System.out.println("Default contructor in class Answer.");
	}
	public Answer(int a) {
		this();
		System.out.println();
		System.out.println("Parameterized contructor in class Answer.");
		System.out.println();
		System.out.println("Your school fee is:"+a+"$");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Answer a = new Answer();
		Answer b = new Answer(100);
		Object n = 1411;
		   System.out.println(n);
		   System.out.println(n.getClass().getName());
	}

}
