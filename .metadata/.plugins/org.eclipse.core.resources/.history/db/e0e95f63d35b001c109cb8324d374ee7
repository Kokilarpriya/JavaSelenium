package com.TechForum.CollectionsHomework;

import java.util.ArrayList;

public class UserMainCodeOddEven {
	public static ArrayList<Integer> generateOddEvenList(ArrayList<Integer> al1, ArrayList<Integer> al2){
		
		ArrayList<Integer> al3 = new ArrayList<Integer>();
		//Array 1 odd elements
		for (int i = 0; i < al2.size(); i++) {

			if (i % 2 == 0) {
				al2.remove(i);
				
			}
		}
		System.out.println();
		System.out.println("array 2 even elements:" + al2);
		
		//Array 2 even elements
		for (int i = 0; i < al2.size(); i++) {

			if (i % 2 != 0) {
				al2.remove(i);
				
			}
		}
		System.out.println();
		System.out.println("array 2 even elements:" + al2);
		return al3;
		
		
	}
	
}
