package com.TechForum.JavaPrograms;

public class Point {
	double x = 0;
	double y = 0;
	double dis = 0;
	public Point() {

	}
	Point(double x, double y) {
		 x = this.x;
		 System.out.println("X in cons ="+x);
		 y = this.y;

	}

	public static void main(String[] args) {
		Point p = new Point();
		Point p1 = new Point(4d,4ds);
		Point p2 = new Point(8,8);
		//double ans = p.distance(p1, p2);
		//System.out.println("Distance between" + "(2,2)," + "(4,4)===>" + ans);
	}

	public double distance(Point p1, Point p2) {
		
		double x1 = p1.x;
		System.out.println(x1);
		double y1 = p1.y;
		double x2 = p2.x;
		double y2 = p2.y;
		dis = Math.sqrt((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1));
		return dis;
	}
}
